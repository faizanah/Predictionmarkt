<script>

const PHRASES = {
    months: {mar: 'March',feb: 'February',aug: 'August',sep: 'September',apr: 'April',jun: 'June',jul: 'July',jan: 'January',may: 'May',nov: 'November',dec: 'December',oct: 'October'},
    weekdays: {wed: 'Wed',sun: 'Sun',thu: 'Thu',mon: 'Mon',fri: 'Fri',tue: 'Tue',sat: 'Sat'},
    dateFormat: "MM/DD/YYYY",
    customRange: "Custom Range",
    apply: "Apply",
    cancel: "Cancel",
    from: "From",
    to: "To",
    weekLabel: "W"
};

$(document).ready(function() {
    var start = moment('20180101');
    var end = moment('20180330');
    var polyglot = new Polyglot({phrases: PHRASES});

    function cb(start, end) {
        $('#reportrange span').html(start.format('MMM D, YYYY') + ' - ' + end.format('MMM D, YYYY'));
    }

    $('#reportrange').daterangepicker({
        startDate: start,
        endDate: end,
        "locale": {
            "format": polyglot.t('dateFormat'),
            "separator": " - ",
            "applyLabel": polyglot.t('apply'),
            "cancelLabel": polyglot.t('cancel'),
            "fromLabel": polyglot.t('from'),
            "toLabel": polyglot.t('to'),
            "customRangeLabel": polyglot.t('customRange'),
            "weekLabel": polyglot.t('weekLabel'),
            "daysOfWeek": [
                polyglot.t('weekdays.sun'),
                polyglot.t('weekdays.mon'),
                polyglot.t('weekdays.tue'),
                polyglot.t('weekdays.wed'),
                polyglot.t('weekdays.thu'),
                polyglot.t('weekdays.fri'),
                polyglot.t('weekdays.sat')
            ],
            "monthNames": [
                polyglot.t('months.jan'),
                polyglot.t('months.feb'),
                polyglot.t('months.mar'),
                polyglot.t('months.apr'),
                polyglot.t('months.may'),
                polyglot.t('months.jun'),
                polyglot.t('months.jul'),
                polyglot.t('months.aug'),
                polyglot.t('months.sep'),
                polyglot.t('months.oct'),
                polyglot.t('months.nov'),
                polyglot.t('months.dec')
            ],
            "firstDay": 1
        },
        ranges: {
            'Last 7 Days': [moment.utc().subtract(6, 'days'), moment.utc()],
            'Last 30 Days': [moment.utc().subtract(30, 'days'), moment.utc()],
            'Last 3 Months': [moment.utc().subtract(3, 'months'), moment.utc()],
            'Last 12 Months': [moment.utc().subtract(12, 'months'), moment.utc()],
            'Year To Date': [moment.utc().startOf('year'), moment.utc()],
            'All Time': ["04-28-2013", moment.utc()]
        }
    }, cb);

    cb(start, end);


  $('#reportrange').on('apply.daterangepicker', function(ev, picker) {
    //do something, like clearing an input
    var startDate = picker.startDate.format('YYYYMMDD');
    var endDate = picker.endDate.format('YYYYMMDD')
    var location = "/currencies/stellar/historical-data/?start=" + startDate + "&end=" + endDate;
    window.location = location
  });  

    renderCurrencyDetailFields(Currency.get());
    runNumberFormatters();
});

Currency.onChange(function (e, currency) {
    renderCurrencyDetailFields(currency);
});

</script>
<script>
            $( document ).ready(function() {
                // GTM Session Set
                var maxExpire = new Date('2038-01-19 00:00:00');
                if (Cookies.get("gtm_session_first") === undefined) {
                    Cookies.set("gtm_session_first", new Date(), {expires: maxExpire})
                }            

                Cookies.set("gtm_session_last", new Date(), {expires: maxExpire})
                
                // Header Banner Prefs
                $(".header-banner-close").on('click', function () {
                    Cookies.set("header-banner-noshow", "1", {expires: 1})
                    $('#header-banner-wrapper').fadeOut();
                    dataLayer.push({
                        'event': 'customEvent',
                        'eventCategory': 'Header Banner',
                        'eventAction': 'Close',
                    });
                });

            });

            (function () {
                var polyglot = new Polyglot({phrases: {
                    cryptocurrencies: "Cryptocurrencies",
                    exchanges: "Exchanges"
                }});

                initQuickSearch({
                    currencyDataURL: 'https://' + STATIC_DOMAIN + '/generated/search/quick_search.json',
                    exchangeDataURL: 'https://' + STATIC_DOMAIN + '/generated/search/quick_search_exchanges.json',
                    currencySuggestionTemplate: function (context) {
                        var template = "/currencies/%25s/";
                        context.url = template.replace('%25s', context.slug);
                        return Handlebars.templates['quick_search'](context);
                    },
                    exchangeSuggestionTemplate: function (context) {
                        var template = "/exchanges/%25s/";
                        context.url = template.replace('%25s', context.slug);
                        return Handlebars.templates['quick_search_exchanges'](context);
                    },
                    currencyLimit: 6,
                    exchangeLimit: 4
                }, polyglot);

                $('[data-language-toggle]').on('click', function () {
                    dataLayer.push({
                        'event': 'customEvent',
                        'eventCategory': 'Language Dropdown',
                        'eventAction': 'Language Selected',
                        'eventLabel': $(this).data('language-toggle')
                    });
                });

                var language = $('[data-language-toggle="' + 'en' + '"]').html();
                $('[data-language-dropdown]').html(language);

                NightMode.initSwitch();
            })();
        </script>
