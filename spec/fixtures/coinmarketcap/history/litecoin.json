                });
            }

            function requestBids(apstagSlots, adUnits, bidTimeout) {
                apstag.fetchBids({
                    slots: apstagSlots,
                    timeout: bidTimeout
                    }, function(bids) {
                        headerBidderBack('a9');
                    });

