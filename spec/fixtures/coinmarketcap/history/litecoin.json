                        }
                      },
                      openx: {
                        bidCpmAdjustment : function(bidCpm){
                          return bidCpm * 0.80;
                        }
                      }                      
                    };

                    pbjs.requestBids({
                        bidsBackHandler: function (bids) {
                            headerBidderBack('prebid');
                        }
                    });
                });
            }

            requestBids(apstagSlots, adUnits, bidTimeout)
            window.setTimeout(function() {
                sendAdserverRequest();
            }, bidTimeout);
        };

        fetchHeaderBids();

